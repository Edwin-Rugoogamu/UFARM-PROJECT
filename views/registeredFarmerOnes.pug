doctype html
head
  meta(charset='UTF-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  link(rel='stylesheet' href='https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css' integrity='sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm' crossorigin='anonymous')
  link(rel='stylesheet' href='../style.css')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Document
div.make
  div.register5  
    .container-fluid
      a(href='./homepageAo')
       button.btn.btn-outline-success(type='button') back
      fieldset
        // Form Name
        legend
          center
            h2
              b Farmer Ones Registered
        br
          
        .container-fluid

        
            table.table
                thead.thead-dark
                    tr
                      th(scope='col') Name
                      //- th(scope='col') Lastname
                      th(scope='col') Email
                      th(scope='col') Role
                      th(scope='col') Unique ID
                      th(scope='col') Gender
                      th(scope='col') Ward
                      th(scope='col') Date
                      th(scope='col') Address
                      th(scope='col') Phone Number
                      th(scope='col') Activities
                      th(scope='col') Edit
                
                tbody
                if data.length 
                    each farmerOne in data
                        tr
                          td #{farmerOne.firstName} #{farmerOne.secondName}
                          td #{farmerOne.email}
                          td #{farmerOne.role}
                          td #{farmerOne.password}
                          td #{farmerOne.gender}
                          td #{farmerOne.user}
                          td #{farmerOne.dobirth}
                          td #{farmerOne.placeOfResidence}
                          td #{farmerOne.phonenumber}
                          td #{farmerOne.activities}    
                          td 
                            a.btn.btn-dark.btn-block(href="/edit_FO/"+farmerOne._id) Edit   
                else 
                    p 
                      h4 There are no Urban Farmers registered yet
